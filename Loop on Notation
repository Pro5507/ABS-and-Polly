        "def abc(n):\n",
        "  intration= 0\n",
        "  for i in range(1, n*1):\n",
        "    for j in range(i, n*1):\n",
        "      print(\"*\", end=\" \")\n",
        "      intration= intration+1\n",
        "    print(\"\")\n",
        "  print(intration)\n",
        "abc(10)\n",
        "abc(20)\n",
        ""
